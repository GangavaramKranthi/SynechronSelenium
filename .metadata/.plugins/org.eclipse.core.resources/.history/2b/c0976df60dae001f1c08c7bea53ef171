package Collections1;

import java.util.ArrayList;

public class arraylistex {

	public static void main(String[] args) {
		//allows duplicate vallues,
				//able to store dynamic data
				//allow nulll values
				// maintains inseration order
				//non synchronized multi threading is allowed if the threads try to modify the data final output is inconsistant

				
				ArrayList <String> list = new ArrayList <String> ();
				
				list.add("jhon");
				list.add("ravi");
				list.add("peter");
				list.add("ravi");
				System.out.println(list);
				
				
			}
	}

}

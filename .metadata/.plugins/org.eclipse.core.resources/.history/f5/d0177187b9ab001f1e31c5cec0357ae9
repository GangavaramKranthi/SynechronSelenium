package corejava;

public class forloop {

	public static void main(String[] args) {
		
		int i =1;
		
		for(int i1=1; i<=10 ; i++) {
			
			System.out.println(i);	
			}
			
//		for(int i=10; i>=1 ; i--) {
//			
//			System.out.println(i);	
//			}
		
		for(int j=1; j<=i ; j++) {
			
			System.out.println(j);	
			}
		}
	

	}



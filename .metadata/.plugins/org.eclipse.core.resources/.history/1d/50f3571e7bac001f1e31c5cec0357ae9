package corejava;

public class hericalsubclass3 extends HericalSuperclass {
	
  public void display3() {
		
		System.out.println("i am in subclass3");
		
	}
	

	public static void main(String[] args) {
 hericalsubclass3 hs =new hericalsubclass3();
		
		//method from parent class(HericalSuperclass)
		hs.display();
		//meethod from child class( hericalsubclass3)
		hs.display3();
	}

}

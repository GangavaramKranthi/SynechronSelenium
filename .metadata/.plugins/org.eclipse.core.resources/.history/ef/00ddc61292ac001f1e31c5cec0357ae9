package corejava;

public class Exceptionex {
	
	public void nulldisplay() {
	
	
	try {
			String s=null;
			
			System.out.println(s.length());
			
		}
	
	catch(Exception e) {
		
		e.printStackTrace();
	}
	
	}
	
	
	public static void main(String[] args) {
		
		
		
		Exceptionex obj=new Exceptionex ();
		obj.nulldisplay();

	}

}



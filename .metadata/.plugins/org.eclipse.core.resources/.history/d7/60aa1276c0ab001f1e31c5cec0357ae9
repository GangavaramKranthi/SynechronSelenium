package corejava;

public class Stringexample {

	public static void main(String[] args) {
		
		String s1="New";
		String s2="Delhi";
		String s3 = "Grater,Bangalore";
		
		System.out.println(s1.equals(s2));//flase
		System.out.println(s1.concat(s2));//newdehil
		System.out.println(s1.substring(1));//ew
		
		System.out.println(s2.substring(2, 4));//lh
		
		
		System.out.println(s1.compareTo(s2));//10 
		
		System.out.println(s1.contains("e"));//check contain represent in the string
		
		System.out.println(s2.replace("e", "a"));
		
		System.out.println(s2.isBlank());//flase
		System.out.println(s2.isEmpty());//flase
		
		String [] arrofstr= s3.split(",");
		
		for (String a:arrofstr) {
			System.out.println(a);
			
		}
		
		System.out.println(s2.join(s2, arrofstr));
		
		String Fruits= String.join(" ","orgne", "apple","banna");
		System.out.println(Fruits);
		
		 String s4= "Panner";
		 String s5 ="butter";
		 
		 System.out.println(s4.compareTo(s5));
	}

}

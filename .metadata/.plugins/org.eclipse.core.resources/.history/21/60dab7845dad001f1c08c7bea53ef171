public class ReadPropertiesFile {

	public static void main(String[] args) throws IOException {


		Properties prop =new Properties();
		
		FileInputStream fin= new FileInputStream("C://Users//Kiosk_user//eclipse-workspace-Kranthi//corejava//Config.Properties");
		
		prop.load(fin);
		

}

	
	
}

package corejava;

public class opertors {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int a= 5;
		int b= 15;
		
		int c = a*b;
		int e = a+b;
		int f =b-a;
		
		System.out.println(c);
		System.out.println(e);
		System.out.println(f);
		System.out.println(b/a);
		
	}

}

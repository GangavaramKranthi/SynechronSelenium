package corejava;

public class swithcase {

	public static void main(String[] args) {

		int num=2;
		
		switch(num) {
		
		case 0:
		System.out.println("The number is 0");
		
		break;
		
		case 1:
			System.out.println("The number is 2");
			
			break;
		
		case 2:
			System.out.println("The number is 2");
			
			break;
		
		default:
			System.out.println("invalid number");
		}
		
	}
}
